#!/bin/bash
./process-file /usr/wiss/sturmju/ros/kinect_vslam/kinect_datasets/bags/kinect_dataset_floor.bag $1 2>&1 | tee process-kinect_dataset_floor.bag.log

./process-file /usr/wiss/sturmju/ros/kinect_vslam/kinect_datasets/bags/public/kinect_dataset_xyz.bag $1 2>&1 | tee process-kinect_dataset_xyz.bag.log
