#!/bin/bash

scripts/evaluate.py --evaldelta 1 \
		../../../work/groundtruth/kinect_dataset_desk-gt.log \
		../../../work/rgbdslam/kinect_desk-30.txt \
		> ../../../work/eval_delta_rgbdslam_desk_30hz.txt
  
scripts/evaluate.py --evaldelay 1 \
		../../../work/groundtruth/kinect_dataset_desk-gt.log \
		../../../work/rgbdslam/kinect_desk-30.txt \
		> ../../../work/eval_delay_rgbdslam_desk_30hz.txt
  
scripts/evaluate.py --evaldelta 1 \
		../../../work/groundtruth/kinect_dataset_xyz-gt.log \
		../../../work/rgbdslam/kinect_xyz_30_hz.txt \
		> ../../../work/eval_delta_rgbdslam_xyz_30hz.txt
  
scripts/evaluate.py --evaldelay 1 \
		../../../work/groundtruth/kinect_dataset_xyz-gt.log \
		../../../work/rgbdslam/kinect_xyz_30_hz.txt \
		> ../../../work/eval_delay_rgbdslam_xyz_30hz.txt
  
# ---

scripts/evaluate.py --evaldelta 1 \
		../../../work/groundtruth/kinect_dataset_desk-gt.log \
		../../../work/fasticp/tf_kinect_dataset_desk-30hz-points-gt.txt \
		> ../../../work/eval_delta_fasticp_desk_30hz.txt
  
scripts/evaluate.py --evaldelay 1 \
		../../../work/groundtruth/kinect_dataset_desk-gt.log \
		../../../work/fasticp/tf_kinect_dataset_desk-30hz-points-gt.txt \
		> ../../../work/eval_delay_fasticp_desk_30hz.txt
  
scripts/evaluate.py --evaldelta 1 \
		../../../work/groundtruth/kinect_dataset_xyz-gt.log \
		../../../work/fasticp/tf_kinect_dataset_xyz-30hz-points-gt.txt \
		> ../../../work/eval_delta_fasticp_xyz_30hz.txt
  
scripts/evaluate.py --evaldelay 1 \
		../../../work/groundtruth/kinect_dataset_xyz-gt.log \
		../../../work/fasticp/tf_kinect_dataset_xyz-30hz-points-gt.txt \
		> ../../../work/eval_delay_fasticp_xyz_30hz.txt
