#!/bin/bash

cd $(rospack find rgbd_benchmark_tools)

ORIGINAL_BAG=~/public_html/dirs/kinect_datasets_org/bags/public/kinect_dataset_rgb_calibration.bag
SYNCED_BAG=/work/sturmju/tmp/freiburg1_calibration-synced.bag
CALIBRATION_BAG=freiburg1/calibration.bag
PROP="--prop freiburg1/Kinect.prop --prop  freiburg1/CheckerBoard.prop" 
LOGFILE=freiburg1/delay.txt

DELAY=0.01
scripts/synchronize.py --delay $DELAY $ORIGINAL_BAG $SYNCED_BAG
scripts/calibrate.py $SYNCED_BAG $CALIBRATION_BAG $PROP

