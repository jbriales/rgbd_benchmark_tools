#!/bin/bash

cd $(rospack find rgbd_benchmark_tools)

datasets="xyz rpy desk desk2 room 360 floor plant teddy rgb_calibration ir_calibration"

bags=$(find /usr/wiss/sturmju/public_html/dirs/rgbd_datasets/freiburg1/*.bag | grep -v hz | grep -v synced)
echo $bags

exit
./scripts/generate_wikitext.py ~/public_html/dirs/rgbd_datasets/freiburg1/index.html $bags


